          <FontSize>12pt</FontSize>
          <Color>#2C3E50</Color>
        </Style>
      </Textbox>
      
      <!-- Financial Summary -->
      <Textbox Name="FinancialSummary">
        <CanGrow>true</CanGrow>
        <KeepTogether>true</KeepTogether>
        <Paragraphs>
          <Paragraph>
            <TextRuns>
              <TextRun>
                <Value>=IIF(Sum(Fields!RevenueAmount.Value) - Sum(Fields!ExpenseAmount.Value) > 0, 
                  "✅ PROFITABLE PERIOD: Your business generated a NET PROFIT of $" + Format(Sum(Fields!RevenueAmount.Value) - Sum(Fields!ExpenseAmount.Value), "N2") + " during this period.",
                  "❌ LOSS PERIOD: Your business incurred a NET LOSS of $" + Format(Abs(Sum(Fields!RevenueAmount.Value) - Sum(Fields!ExpenseAmount.Value)), "N2") + " during this period.")</Value>
              </TextRun>
            </TextRuns>
            <Style>
              <TextAlign>Left</TextAlign>
            </Style>
          </Paragraph>
        </Paragraphs>
        <Top>0.85in</Top>
        <Left>0.25in</Left>
        <Height>0.15in</Height>
        <Width>11in</Width>
        <Style>
          <BackgroundColor>=IIF(Sum(Fields!RevenueAmount.Value) - Sum(Fields!ExpenseAmount.Value) > 0, "#D5F4E6", "#FFE6E6")</BackgroundColor>
          <Border>
            <Color>=IIF(Sum(Fields!RevenueAmount.Value) - Sum(Fields!ExpenseAmount.Value) > 0, "#27AE60", "#E74C3C")</Color>
            <Style>Solid</Style>
          </Border>
          <FontWeight>Bold</FontWeight>
          <FontSize>10pt</FontSize>
          <Color>=IIF(Sum(Fields!RevenueAmount.Value) - Sum(Fields!ExpenseAmount.Value) > 0, "#27AE60", "#E74C3C")</Color>
        </Style>
      </Textbox>
      
